    "import sympy as sp\n",
    "from sympy.physics.matrices import mgamma\n",
    "from sympy.functions.elementary.complexes import conjugate"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "7a26f5f6-0614-4ff1-affc-aeb4b708b84f",
   "metadata": {},
   "outputs": [],
   "source": [
    "a, b, c, d, e, f, g, h, i, j, k, l, m, n, o, p = sp.symbols('a b c d e f g h i j k l m n o p')\n",
    "C = sp.Matrix([[a, b, c, d], [e, f, g, h], [i, j, k, l], [m, n, o, p]])\n",
    "# C = sp.Matrix([[a, b], [c, d]])\n",
    "A = sp.MatrixSymbol('A', 4, 4)\n",
    "g0 = sp.Matrix([[1, 0], [0, -1]])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "ddc6c4bf-823e-4614-a6bf-fd99eefe83ef",
   "metadata": {},
   "outputs": [],
   "source": [
    "Ci = sp.Inverse(C)"
   ]
  },
